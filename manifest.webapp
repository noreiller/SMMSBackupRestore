{
  "version": "0.1.0",
  "name": "SMMSBackupRestore",
  "description": "Backup and restore SMS and MMS.",
  "launch_path": "/index.html",
  "icons": {
    "16": "/img/icons/icon16x16.png",
    "48": "/img/icons/icon48x48.png",
    "60": "/img/icons/icon60x60.png",
    "128": "/img/icons/icon128x128.png"
  },
  "developer": {
    "name": "Aur√©lien MANCA",
    "url": "https://www.amanca.fr/"
  },
  "type": "certified",
  "permissions": {
    "device-storage:sdcard": {
      "access": "readcreate"
    },
    "sms": {}
  },
  "installs_allowed_from": [
    "*"
  ],
  "locales": {
    "en-US": {
      "name": "SMMSBackupRestore",
      "description": "Backup and restore SMS and MMS.",
      "permissions": {
        "device-storage:sdcard": {
          "description": "Required to save the backup"
        },
        "sms": {
          "description": "Required to be able to backup and restore the SMS and MMS"
        }
      }
    },
    "fr": {
      "name": "SMMSBackupRestore",
      "description": "Sauvegarde et restaure les SMS et MMS.",
      "permissions": {
        "device-storage:sdcard": {
          "description": "Requis pour pouvoir enregistrer la sauvegarde"
        },
        "sms": {
          "description": "Requis pour pouvoir sauvegarder et restaurer les SMS et MMS"
        }
      }
    }
  },
  "default_locale": "en-US",
  "activities": {
    "open": {
      "href": "/index.html",
      "disposition": "window",
      "filters": {
        "type": ["application/json"]
      }
    },
    "share": {
      "href": "/index.html",
      "disposition": "window",
      "filters": {
        "type": ["image/*"]
      }
    }
  }
}
